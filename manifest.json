{
  "manifest_version": 1,

  "name": "Magic Wall",
  "description": "This extension changes chrome wallpaper randomly",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Change the wallpaper"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}